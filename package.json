{
  "name": "@xsoft/storybook-ai-generator",
  "version": "1.0.4",
  "description": "A fully automated Storybook file generator powered by Node.js, TypeScript, ts-morph, and LLM reasoning.",
  "main": "dist/cli/index.js",
  "private": false,
  "bin": {
    "storybook-ai": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "generate": "node dist/cli/index.js generate",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "validate": "tsc --noEmit && npm run lint"
  },
  "keywords": [
    "storybook",
    "storybook-ai-generator",
    "langchain",
    "gemini",
    "openai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xuanbui-epam/storybook-ai-generator"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "README.MD"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.18.0",
    "@langchain/core": "^1.1.2",
    "@langchain/google-genai": "^2.0.2",
    "@langchain/openai": "^1.1.3",
    "@vue/compiler-sfc": "^3.5.13",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "glob": "^13.0.0",
    "openai": "^4.73.0",
    "ts-morph": "^27.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
